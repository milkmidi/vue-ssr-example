{
  "name": "vue-ssr-example",
  "version": "1.0.0",
  "description": "vue ssr example",
  "main": "index.js",
  "scripts": {
    "start": "node_modules/.bin/webpack-dev-server --config build/webpack.client.config.js --hot --devtool eval --progress",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milkmidi/vue-ssr-example.git"
  },
  "author": "milkmidi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/milkmidi/vue-ssr-example/issues"
  },
  "homepage": "https://github.com/milkmidi/vue-ssr-example#readme",
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-env": "^2.4.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-stage-3": "^6.22.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.2.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-html": "^3.0.0",
    "eslint-plugin-import": "^2.6.1",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.29.0",
    "vue": "^2.3.4",
    "vue-loader": "^13.0.1",
    "vue-server-renderer": "^2.3.4",
    "vue-template-compiler": "^2.3.4",
    "webpack": "^3.1.0",
    "webpack-merge": "^4.1.0"
  },
  "devDependencies": {
    "plop": "^1.8.0",
    "webpack-dev-server": "^2.5.0"
  }
}
